<div>
  <div id="logo-root"
       data-logo-path="<%= image_path('logo4.png') %>">
  </div>
  <div id="navbar-root"
       data-spaces-areas="<%= @spaces_areas.to_json %>"
       data-stores-areas="<%= @stores_areas.to_json %>">
  </div>
  <div id="page-tagline-root"
       data-category="<%= @page_category %>"
       data-filter-type="<%= @filter_type %>"
       data-filter-value="<%= @filter_value %>">
  </div>
  <script type="application/json" id="stores-data">
    <%= @stores.to_json.html_safe %>
  </script>
  <div id="stores-list-root"
       data-title="<%= "#{@filter_value}" %>"
       style="min-height: 200px;">
  </div>
  <div id="footer-root"
       data-instagram-image-path="<%= image_path('instagram_logo.png') %>">
  </div>
</div>
